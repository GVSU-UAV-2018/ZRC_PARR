<?xml version="1.0"?>
<block>
  <name>collar_detect</name>
  <key>collar_detect_collar_detect</key>
  <category>collar_detect</category>
  <import>import collar_detect</import>
  <make>collar_detect.collar_detect()</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>SNR</name>
    <key>SNR</key>
    <type>float</type>
  </param>

  <sink>
    <name>in</name>
    <type>float</type>
    <vlen>512</vlen>
  </sink>

</block>
